{
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "https://esm.sh/react@18",
    "lib": [
      "deno.window",
      "deno.ns"
    ],
    "allowJs": true,
    "strict": false,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "skipLibCheck": true
  },
  "imports": {
    "xhr": "https://deno.land/x/xhr@0.1.0/mod.ts",
    "std/": "https://deno.land/std@0.168.0/",
    "@supabase/supabase-js": "npm:@supabase/supabase-js@2.39.3",
    "@google/generative-ai": "npm:@google/generative-ai@0.21.0"
  },
  "lint": {
    "rules": {
      "exclude": [
        "no-explicit-any"
      ]
    }
  }
}